{
  "name": "anycodecli",
  "version": "0.1.0",
  "private": true,
  "type": "module",
  "bin": {
    "anycodecli": "./bin/anycodecli.mjs"
  },
  "scripts": {
    "typecheck": "tsc --noEmit",
    "build": "rm -rf dist && tsc -p tsconfig.json",
    "dev": "tsx src/index.ts",
    "test": "vitest run",
    "check-naming": "tsx scripts/check-naming.ts",
    "ci": "npm run typecheck && npm run test && npm run check-naming"
  },
  "dependencies": {},
  "devDependencies": {
    "@types/node": "^22.13.10",
    "tsx": "^4.20.6",
    "typescript": "^5.7.3",
    "vitest": "^3.2.4"
  }
}
